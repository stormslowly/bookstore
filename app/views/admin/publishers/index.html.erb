<h1>Listing admin_publishers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_publishers.each do |admin_publisher| %>
      <tr>
        <td><%= admin_publisher.name %></td>
        <td><%= link_to 'Show', admin_publisher %></td>
        <td><%= link_to 'Edit', edit_publisher_path(admin_publisher) %></td>
        <td><%= link_to 'Destroy', admin_publisher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publisher', new_publisher_path %>
